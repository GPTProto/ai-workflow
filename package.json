{
  "name": "video-workflow",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "author": "Video Workflow Team",
  "description": "AI-powered video generation workflow tool",
  "scripts": {
    "dev": "next dev",
    "build:prod": "next build",
    "start": "next start",
    "lint": "eslint",
    "deploy": "npx tsx deploy.ts deploy",
    "rollback": "npx tsx deploy.ts rollback",
    "versions": "npx tsx deploy.ts list",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build:prod && npm run electron:pack",
    "electron:pack": "electron-builder --config electron-builder.json",
    "electron:pack:mac": "electron-builder --config electron-builder.json --mac",
    "electron:pack:win": "electron-builder --config electron-builder.json --win",
    "electron:pack:linux": "electron-builder --config electron-builder.json --linux"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@supabase/supabase-js": "^2.86.2",
    "@types/fluent-ffmpeg": "^2.1.28",
    "ali-oss": "^6.23.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fluent-ffmpeg": "^2.1.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.555.0",
    "next": "16.0.7",
    "proxy-agent": "^5.0.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "reactflow": "^11.11.4",
    "sharp": "^0.34.5",
    "ssh2": "^1.17.0",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/ali-oss": "^6.16.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ssh2": "^1.15.5",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
}
